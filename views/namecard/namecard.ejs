<h1>namecard</h1>
<table class="table table-sm table-bordered" style="width : 600px">
    <%
        var i = 0;
        while(i < results.length) {
    %>
        <tr><td class="td td-sm">이름</td><td>회사</td><td>전화번호</td><td>주소</td></tr>
        <tr><td class="td td-sm"><%=results[i].name%></td><td><%=results[i].com%></td><td><%=results[i].tel%></td><td><%=results[i].address%></td></tr>
    <% 
        i++;
    }
    %>
</table>